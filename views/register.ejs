  <body>
    <h1>Register</h1>
    <div class="" id="register-form">
      <form method="POST" action="/register">
        <br />
        User Name : <input type="text" name="name" required /> <br />
        email: <input type="email" name="user_email" required /><br />
        Password : <input type="password" name="password" />
        <br />
        Address :
        <input type="text" name="address" /><br />
        Contact no : <input type="number" name="contact_no" required /><br />

        <input type="submit" />
      </form>
    </div>

    <a href="/login">Log in</a>
    <br />
    <a href="/">Home</a>
  </body>
</html>
